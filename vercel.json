{
  "version": 2,
  "functions": {
    "api/server.js": {
      "runtime": "@vercel/node@2"
    }
  },
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/server.js" },
    { "src": "^/favicon.ico$", "dest": "/public/favicon.ico" },
    { "src": "^/(.*\\.(css|js|ico|png|jpg|jpeg|gif|svg))$", "dest": "/public/$1" },
    { "src": "^/$", "dest": "/public/index.html" },
    { "src": "^/(.*)$", "dest": "/public/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}